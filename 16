а)using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("5 10");
        Console.WriteLine("7 см");
    }
}

б)using System;

class Program
{
    static void Main()
    {
        Console.Write("Введите значение для t: ");
        string t = Console.ReadLine();
        
        Console.Write("Введите значение для v: ");
        string v = Console.ReadLine();
        
        Console.WriteLine($"100 {t}");
        Console.WriteLine($"1949 {v}");
    }
}

в)using System;

class Program
{
    static void Main()
    {
        Console.Write("Введите значение для x: ");
        string x = Console.ReadLine();
        
        Console.Write("Введите значение для y: ");
        string y = Console.ReadLine();
        
        Console.WriteLine($"{x} 25");
        Console.WriteLine($"{x} {y}");
    }
}
